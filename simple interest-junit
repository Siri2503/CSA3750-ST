package si2;
import static org.junit.Assert.assertTrue;
import java.util.Scanner;
class si
{
       public static void main(String[] args)
       {
              
              float P=600;
              float R=600;
              float T=1;
              float SI=(P*T*R)/100;
              System.out.println("Simple interest = "+SI); 
              assertTrue(3600==SI);
} 
}
